job(  name      : ENDE;
      comment   : Schliessen des SGBD;
   ) 
{
#asm
					clear		S0
					pary		S0
					jnz			exitpar
					clear		S0
					move		S0,"CMD_TEST2_EXIT"
exitpar:
					shmset		"ID",S0

					strcmp		S0,"EXIT_EXCEPTION"
					jz			exitnoexcept1
					clrt
					settmr		#$FFFFFFFF.L
					sett		#11
					eerr
exitnoexcept1:
					strcmp		S0,"BOTH_EXCEPTION"
					jz			exitnoexcept2
					clrt
					settmr		#$FFFFFFFF.L
					sett		#12
					eerr
exitnoexcept2:
#endasm
}

